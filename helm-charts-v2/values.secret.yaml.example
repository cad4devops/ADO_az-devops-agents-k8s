## Example values (do NOT commit real secrets)
# Copy this to values.secret.yaml and replace the placeholders with base64-encoded values

secret:
  name: sh-agent-secret-003
  data:
    AZP_URL: "${BASE64_ENCODED_AZP_URL}"
    AZP_TOKEN: "${BASE64_ENCODED_PAT}"
    AZP_POOL_LINUX: "${BASE64_ENCODED_POOL_NAME}"
    AZP_POOL_WINDOWS: "${BASE64_ENCODED_POOL_NAME}"

deploy:
  linux: true
  windows: true

image:
  linux: youracr.azurecr.io/linux-sh-agent:latest
  windows: youracr.azurecr.io/windows-sh-agent:2022

acrs:
  name: ''
  username: ''
  password: ''
